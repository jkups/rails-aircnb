          <% end %>

      <%= link_to "Reservations", reservations_path, class: "navbar-brand mb-0 h1"%>
      <%= link_to "Users", users_path, class: "navbar-brand mb-0 h1"%>
      <%= link_to "Images", images_path, class: "navbar-brand mb-0 h1"%>
      <%= link_to "Properties", properties_path, class: "navbar-brand mb-0 h1"%>
